      },
      {
        "subsystem": "notification",
        "components": [
          { "component": "common_event_service", "features":[ "common_event_service_with_graphics=false" ] },
          { "component": "eventhandler", "features":[] }
        ]
      },
      {
        "subsystem": "systemabilitymgr",
        "components": [
          { "component": "samgr", "features":[] },
          { "component": "safwk", "features":[] }
        ]
      },
      {
        "subsystem": "deviceprofile",
        "components": [
          { "component": "device_info_manager", "features":[] }
        ]
      },
      {
        "subsystem": "distributeddatamgr",
        "components": [
          { "component": "datamgr_service", "features":[] },
          { "component": "kv_store", "features":[] },          
          { "component": "distributedfilejs", "features":[] },
          { "component": "relational_store", "features":[] },
          { "component": "preferences", "features":[] },
          { "component": "data_share", "features":[] },
          { "component": "native_appdatamgr", "features":[ "native_appdatamgr_rdb_support_icu=false" ] },
          { "component": "data_object", "features":[] }
        ]
      },
      {
        "subsystem": "filemanagement",
        "components": [
          { "component": "storage_service", "features":[ "storage_service_graphic=false" ] }
        ]
      },
      {
        "subsystem": "global",
        "components": [
          { "component": "resource_management", "features":[ "resource_management_support_icu=false" ] },
          { "component": "i18n", "features":[] }
        ]
      },
      {
        "subsystem": "device_arm_virt",
        "components": [
          { "component": "qemu_arm_linux_chipset", "features":[] }
        ]
      },
      {
        "subsystem": "developtools",
        "components": [
          { "component": "bytrace", "features":[] },
          { "component": "profiler", "features":["profiler_SmartPerf=false"] },
          { "component": "hdc", "features":[] },
          { "component": "syscap_codec", "features":[] }
        ]
      },
      {
        "subsystem": "securec",
        "components": [
          { "component": "thirdparty_bounds_checking_function", "features":[] }
        ]
      },
      {
        "subsystem": "procrank",
        "components": [
          { "component": "procrank", "features":[] }
        ]
      },
      {
        "subsystem": "hdf",
        "components": [
          { "component": "hdf_core", "features":[] }
        ]
      }
    ]
  }
